{
  "name": "primex-grup-chat",
  "version": "9.66.0",
  "description": "PrimeX Grup - Ultimate Realtime Chat System with All Features",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --verbose",
    "build": "npm run build-css && npm run build-js",
    "build-css": "node-sass public/assets/css/main.scss public/assets/css/main.css",
    "build-js": "browserify public/assets/js/app.js -o public/assets/js/bundle.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t primex-chat .",
    "docker:run": "docker-compose up -d",
    "backup": "node src/utils/backup.js",
    "stats": "node src/services/analyticsService.js",
    "clean": "rm -rf node_modules && npm cache clean --force",
    "setup": "npm install && node src/config/setup.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs primex-chat"
  },
  "keywords": [
    "chat",
    "realtime",
    "socket.io",
    "nodejs",
    "mongodb",
    "primex",
    "whatsapp-clone",
    "messenger"
  ],
  "author": {
    "name": "Epan",
